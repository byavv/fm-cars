{
  "name": "carsms",
  "version": "1.0.0",
  "author": "Viacheslav Aksenchyk <aksenchyk.v@gmail.com>",
  "main": "server/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "test": "cross-env NODE_ENV=test DEBUG=test istanbul cover node_modules/mocha/bin/_mocha -- -R spec test/*.spec.js",
    "dev": "cross-env NODE_ENV=development DEBUG=ms:* node .",
    "test-cov": "cross-env NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec test/*.spec.js && cat ./coverage/lcov.info | ./node_modules/.bin/codecov && $(CI_ENV)",
    "clean": "npm run clean:coverage && npm run clean:temp && npm run clean:build",
    "clean:coverage": "rm -rf ./coverage",
    "clean:temp": "rm -rf ./tmp/*",
    "clean:build": "rm -rf ./build/*",
    "serve": "nodemon .",
    "pm2": "cross-env NODE_ENV=production DEBUG=ms:* pm2 start . --name=ms-cars -i max --no-daemon"
  },
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "async": "^2.0.1",
    "body-parser": "^1.15.1",
    "codecov.io": "^0.1.6",
    "cors": "^2.5.2",
    "cross-env": "^2.0.0",
    "debug": "^2.2.0",
    "etcd-registry": "^2.1.1",
    "helmet": "^0.14.0",
    "loopback": "^2.28.0",
    "loopback-boot": "^2.18.1",
    "loopback-component-explorer": "^2.5.0",
    "loopback-component-storage": "^1.8.1",
    "loopback-connector-mongodb": "1.13.0",
    "loopback-datasource-juggler": "^2.46.0",
    "loopback-ds-timestamp-mixin": "^3.2.4",
    "sift": "^3.2.1",
    "wascally": "^0.2.10",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "jshint": "^2.5.6",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nsp": "^2.1.0",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0"
  },
  "license": "MIT",
  "repository": {
    "type": "",
    "url": ""
  },
  "description": "cars api microservice"
}


